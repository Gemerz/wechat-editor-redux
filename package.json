    "redux-devtools-log-monitor": "^1.0.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
